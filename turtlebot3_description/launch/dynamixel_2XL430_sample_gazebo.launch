<launch>
  <arg name="model" default="turtlebot3_big_wheel"/>
  <arg name="x_pos" default="0.0"/>
  <arg name="y_pos" default="0.0"/>
  <arg name="z_pos" default="0.0"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find turtlebot3_gazebo)/worlds/empty.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <param name="robot_description" command="$(find xacro)xacro $(find turtlebot3_description)/urdf/dynamixel_2XL430_sample.urdf.xacro" />

  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model $(arg model) -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -param robot_description" />
  <!-- <node name="spawn_athean_model" pkg="gazebo_ros" type="spawn_model" args="-x 0 -y 0 -z 0 -R 0 -P 0 -Y 0 -unpause -urdf -param robot_description -model $(arg model)"/> -->
</launch>
